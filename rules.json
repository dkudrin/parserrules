{
  "remove": {
    "elements": {
      "bySelector": {
        "htmlTags": [ "script", "noscript", "style", "link", "svg", "canvas" ],
        "html5Tags": [ "footer", "aside", "nav", "stickyShare", "amp-timeago", "amp-facebook-like", "header" ],
        "forms": [ "form", "fieldset", "button", "select", "input", "textarea" ],
        "embed": [ "embed", "object" ],
        "templates": [ "template" ],
        "hidden": [ "[style*=\"display:none\"]" ],
        "empty": [ "*:not(img):empty" ],
        "other": [ "*[class*=\"examle-class-substring\"]"],
        "share": ["*[data-stickya*=\"stickyShare\"]"],
        "nav_links": ["*[itemtype*=BreadcrumbList]", "menu"],
        "comments": ["*[data-categoryid=\"Comments\"]"],
        "more": ["*[items=\"commentCountArr\"]"]
      },
      "ifClassOrId": {
        "contains": [ "-ad-", "banner", "breadcrumbs", "combx", "comment", "community",
                     "cover-wrap", "disqus", "extra", "foot", "header", "legends", "menu",
                     "related", "remark", "replies", "rss", "shoutbox", "sidebar", "skyscraper",
                     "social", "sponsor", "supplemental", "ad-break", "agegate", "pagination", 
                     "pager", "popup", "yom-remote" ],
        "starts": [],
        "ends": []
      }
    },
    "attributes": {
      "inlineJs": [ "onclick", "onerror" ],
      "inlineCss": [ "style", "width", "height" ],
      "ids": [ "id", "class" ]
    }
  },
  "select": {    
    "metatags": {
      "title" :  {
        "byTagProps": [
          "dc:title",
          "dcterm:title",
          "og:title",
          "weibo:article:title",
          "weibo:webpage:title",
          "title",
          "twitter:title"
        ],
        "byTagNames": [
          "title"
        ]
      },
      "description": {
        "byTagProps": [
          "dc:description",
          "dcterm:description",
          "og:description",
          "weibo:article:description",
          "weibo:webpage:description",
          "description",
          "twitter:description"
        ]
      },
      "byline": {
        "byTagProps": [
          "dc:creator",
          "dcterm:creator",
          "article:author_name"
        ],
        "byTagNames": [
          "author"
        ]
      },
      "publishedTime": {
        "byTagProps": [
          "article:published_time"
        ]
      }
    }
  }
}
